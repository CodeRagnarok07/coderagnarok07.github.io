---
import data from "@/content/i18n/es/services.json";
import { Card, CardGrid } from "@astrojs/starlight/components";



---

<CardGrid stagger>
    {
        data.items.map((v) => (
            <Card title={v.title} icon={v.icon as "open-book"}>
                {v.content}
            </Card>
        ))
    }
</CardGrid>
