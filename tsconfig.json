{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "./",
    "paths": {
      "@consts": [
        "src/consts.ts"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@util/*": [
        "src/util/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ]
    }
  }
}