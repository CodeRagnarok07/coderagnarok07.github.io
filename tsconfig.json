{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "./",
    "paths": {
      "@consts": [
        "src/locale/consts.ts"
      ],
      "@ui": [
        "src/components/ui/*"
      ],
      "@/*": [
        "src/*"
      ],
      
    },
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],

  }
}